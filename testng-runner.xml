<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<!--Different combinations: a  full package, a full class from a package, a test case from a class -->
<suite name="Eurotech Automation" verbose="0">
    <test name="Eurotech Smoke Test">
        <packages>
            <package name="com.eurotech.tests.day12_actions_JS"></package>
        </packages>
        <classes>
            <class name="com.eurotech.tests.Day15_POM.PositiveLoginTest"></class>
            <class name="com.eurotech.tests.day12_actions_JS.ActionTest">
                <methods>
                    <exclude name="dragAndDrop"></exclude>
                </methods>
            </class>
        </classes>
    </test>
</suite>